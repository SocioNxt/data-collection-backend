{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js",
        "headers": {
            "Access-Control-Allow-Credentials": "true",
            "Access-Control-Allow-Origin": "https://data-collection-admin-fl7163qq4.vercel.app",
            "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS",
            "Access-Control-Allow-Headers": "Content-Type, Authorization"
        }
      }
    ]
  }
  